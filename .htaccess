AddDefaultCharset utf-8

RewriteEngine on   
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php
<Files "data.json">
  Order Deny,Allow
  Deny from all
</Files>
<Files "main.js">
  Order Deny,Allow
  Deny from all
</Files>
<Files "jquery.js">
  Order Deny,Allow
  Deny from all
</Files>